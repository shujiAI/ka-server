<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <groupId>com.shujiai.ka</groupId>
    <artifactId>shujiai-ka-facade</artifactId>
    <version>1.0.0-SNAPSHOT</version>

    <name>shujiai-ka-facade</name>
    <description>Demo project for Spring Boot</description>

    <properties>
        <maven.compiler.source>1.8</maven.compiler.source>
        <maven.compiler.target>1.8</maven.compiler.target>

        <shujiai-base.version>2.0.0-SNAPSHOT</shujiai-base.version>
        <lombok.version>1.18.20</lombok.version>
        <easyexcel.verson>3.3.2</easyexcel.verson>
    </properties>

    <dependencies>
        <dependency>
            <groupId>com.shujiai</groupId>
            <artifactId>shujiai-base</artifactId>
            <version>${shujiai-base.version}</version>
        </dependency>

        <dependency>
            <groupId>org.projectlombok</groupId>
            <artifactId>lombok</artifactId>
            <optional>true</optional>
            <version>${lombok.version}</version>
        </dependency>

        <dependency>
            <groupId>com.alibaba</groupId>
            <artifactId>easyexcel</artifactId>
            <version>${easyexcel.verson}</version>
        </dependency>
    </dependencies>


</project>
